{
    "manifest_version": 2,
  
    "name": "Id Token Grabber",
    "description": "Copies Id Token for Cognito to your clipboard",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab"
     ]
}